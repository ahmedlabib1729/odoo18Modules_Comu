<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Enrollment Application Sequence -->
        <record id="seq_enrollment_application" model="ir.sequence">
            <field name="name">Enrollment Application Sequence</field>
            <field name="code">quran.enrollment.application</field>
            <field name="prefix">RQC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Student Sequence -->
        <record id="seq_student" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">quran.student</field>
            <field name="prefix">STD/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Class Sequence -->
        <record id="seq_class" model="ir.sequence">
            <field name="name">Class Sequence</field>
            <field name="code">quran.class</field>
            <field name="prefix">CLS/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_session_meeting" model="ir.sequence">
            <field name="name">Session Meeting Sequence</field>
            <field name="code">quran.session.meeting</field>
            <field name="prefix">QRN-MTG-%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>