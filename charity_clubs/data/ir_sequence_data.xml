<odoo>
    <data noupdate="1">
        <!-- Sequence for Registration Numbers -->
        <record id="seq_program_registration" model="ir.sequence">
            <field name="name">تسجيل البرنامج</field>
            <field name="code">program.registration</field>
            <field name="prefix">REG/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range">True</field>
        </record>
    </data>
</odoo>